<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.wikicrimes.model.FonteExterna" table="TB_FNE_FONTE_EXTERNA" lazy="true">
        <id name="idFonteExterna" column="FNE_IDFONTE_EXTERNA" unsaved-value="null">
            <generator class="increment"/>
        </id>
        <property name="nome" column="FNE_NOME" type="java.lang.String" />
        <property name="dataHoraRegistro" column="FNE_DATA_HORA_REGISTRO" type="java.util.Date"/>
        <property name="latitude" column="FNE_LATITUDE" type="java.lang.Double"/>
        <property name="longitude" column="FNE_LONGITUDE" type="java.lang.Double"/>
    </class>
</hibernate-mapping>
