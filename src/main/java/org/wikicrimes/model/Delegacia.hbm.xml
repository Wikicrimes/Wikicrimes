<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.wikicrimes.model.Delegacia" table="TB_DEL_DELEGACIA" lazy="true">
        <id name="idDelegacia" column="DEL_IDDELEGACIA" unsaved-value="null">
            <generator class="increment"/>
        </id>
        <property name="descricao" column="DEL_DESCRICAO" type="java.lang.String" not-null="true"/>
        <property name="latitude" column="DEL_LATITUDE" type="java.lang.Double" not-null="true"/>
        <property name="longitude" column="DEL_LONGITUDE" type="java.lang.Double" not-null="true"/>
        <property name="endereco" column="DEL_ENDERECO" type="java.lang.String" not-null="true"/>
        <property name="cidade" column="DEL_CIDADE" type="java.lang.String" not-null="true"/>
        <property name="estado" column="DEL_ESTADO" type="java.lang.String" not-null="true"/>
        <property name="pais" column="DEL_PAIS" type="java.lang.String" not-null="true"/>
        <property name="telefone" column="DEL_TELEFONE" type="java.lang.String" not-null="true"/>
        <property name="tipoDelegacia" column="DEL_TIPO" type="java.lang.Integer" not-null="true"/>
        <property name="chave" column="DEL_CHAVE" type="java.lang.String" not-null="true"/>
       

    </class>
</hibernate-mapping>
