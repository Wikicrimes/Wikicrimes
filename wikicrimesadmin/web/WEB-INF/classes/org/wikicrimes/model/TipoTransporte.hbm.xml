<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.wikicrimes.model.TipoTransporte" table="TB_TTR_TIPO_TRANSPORTE" lazy="true">
        <id name="idTipoTransporte" column="TTR_IDTIPO_TRANSPORTE" unsaved-value="null">
            <generator class="increment"/>
        </id>
        <property name="nome" column="TTR_NOME" type="java.lang.String" not-null="true"/>
        <property name="descricao" column="TTR_DESCRICAO" type="java.lang.String"/>
    </class>
</hibernate-mapping>
