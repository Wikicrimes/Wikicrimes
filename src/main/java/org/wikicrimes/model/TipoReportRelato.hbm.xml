<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.wikicrimes.model.TipoReportRelato" table="TB_TRR_TIPO_REPORT_RELATO" lazy="true">
        <id name="idTipoReportRelato" column="TRR_IDTIPO_REPORT_RELATO" unsaved-value="null">
            <generator class="increment"/>
        </id>
        <property name="nome" column="TRR_NOME" type="java.lang.String" not-null="true"/>
        <property name="descricao" column="TRR_DESCRICAO" type="java.lang.String"/>
    </class>
</hibernate-mapping>
